%% \mintintabx allows mintinline in tabularx environments
% !use XeLaTeX compiler
% from https://github.com/gpoore/minted/issues/162#issuecomment-299842714
\makeatletter
\def\mintintabx#1#2{%
\ifx\@footnotetext\TX@trial@ftn
\detokenize{#2}%
\else
\mintinline{#1}{#2}%
\fi}
\makeatother